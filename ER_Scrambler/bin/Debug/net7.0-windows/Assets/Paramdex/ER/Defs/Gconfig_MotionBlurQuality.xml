<?xml version="1.0" encoding="utf-8"?>
<PARAMDEF XmlVersion="2">
  <ParamType>CS_MOTION_BLUR_QUALITY_DETAIL</ParamType>
  <DataVersion>2</DataVersion>
  <BigEndian>False</BigEndian>
  <Unicode>True</Unicode>
  <FormatVersion>203</FormatVersion>
  <Fields>
    <Field Def="u8 Enabled = 1">
      <DisplayName>Motion Blur Enabled</DisplayName>
      <Enum>ON_OFF</Enum>
      <Description>Motion blur Enabled</Description>
      <DisplayFormat>%u</DisplayFormat>
      <EditFlags>None</EditFlags>
      <Maximum>1</Maximum>
    </Field>
    <Field Def="u8 ombEnabled = 1">
      <DisplayName>Object Motion Blur Enabled</DisplayName>
      <Enum>ON_OFF</Enum>
      <Description>OMB (object motion blur) Enabled</Description>
      <DisplayFormat>%u</DisplayFormat>
      <EditFlags>None</EditFlags>
      <Maximum>1</Maximum>
    </Field>
    <Field Def="u8 forceScaleVelocityBuffer = 1">
      <DisplayName>Force Scale Velocity Buffer</DisplayName>
      <Enum>ON_OFF</Enum>
      <Description>Decrease the resolution of the velocity buffer used internally. Not effective if you are not using precision velocity buffers</Description>
      <DisplayFormat>%u</DisplayFormat>
      <EditFlags>None</EditFlags>
      <Maximum>1</Maximum>
    </Field>
    <Field Def="u8 cheapFilterMode">
      <DisplayName>Cheap Filter Mode</DisplayName>
      <Enum>ON_OFF</Enum>
      <Description>Normally processed by the Reconstruction filter, but downgraded to a lighter process</Description>
      <DisplayFormat>%u</DisplayFormat>
      <EditFlags>None</EditFlags>
      <Maximum>1</Maximum>
      <SortID>20</SortID>
    </Field>
    <Field Def="s32 sampleCountBias = -2">
      <DisplayName>Sample Count Bias</DisplayName>
      <Description>Give an offset to the sample count * Set to a multiple of 2</Description>
      <EditFlags>None</EditFlags>
      <Minimum>-4</Minimum>
      <Maximum>2</Maximum>
      <SortID>10</SortID>
    </Field>
    <Field Def="s32 recurrenceCountBias">
      <DisplayName>Recurrence Count Bias</DisplayName>
      <Description>Give an offset to the number of recursive blurs</Description>
      <EditFlags>None</EditFlags>
      <Minimum>-1</Minimum>
      <Maximum>1</Maximum>
      <SortID>10</SortID>
    </Field>
    <Field Def="f32 blurMaxLengthScale = 0.75">
      <DisplayName>Blur Max Length Scale</DisplayName>
      <Description>Scale value for blur maximum length parameter</Description>
      <EditFlags>None</EditFlags>
      <Minimum>0</Minimum>
      <Maximum>1</Maximum>
      <Increment>0.1</Increment>
      <SortID>10</SortID>
    </Field>
  </Fields>
</PARAMDEF>