      <Maximum>9999999</Maximum>
      <SortID>5000</SortID>
    </Field>
    <Field Def="f32 PlayerLigntScale = 1">
      <DisplayName>Player Light Scale</DisplayName>
      <Description>Specify the scale to be applied to the PC and PC horse resident light source on this map (SEQ16562).</Description>
      <EditFlags>None</EditFlags>
      <Minimum>0.1</Minimum>
      <Maximum>10</Maximum>
      <Increment>1</Increment>
      <SortID>121</SortID>
    </Field>
    <Field Def="u8 IsEnableTimezonnePlayerLigntScale = 1">
      <DisplayName>Enable Timezone Player Light Scale</DisplayName>
      <Enum>BOOL_CIRCLECROSS_TYPE</Enum>
      <Description>Does this map change the Player light intensity scale depending on the PC, PC horse resident light source, and time zone? (SEQ 16562)</Description>
      <EditFlags>None</EditFlags>
      <Maximum>1</Maximum>
      <SortID>122</SortID>
    </Field>
    <Field Def="u8 isDisableAutoCliffWind">
      <DisplayName>Disable Automatic Cliff Wind</DisplayName>
      <Enum>BOOL_CIRCLECROSS_TYPE</Enum>
      <Description>Do you want to disable automatic cliff wind SE? (SEQ15729)</Description>
      <EditFlags>None</EditFlags>
      <Maximum>1</Maximum>
      <SortID>114</SortID>
    </Field>
