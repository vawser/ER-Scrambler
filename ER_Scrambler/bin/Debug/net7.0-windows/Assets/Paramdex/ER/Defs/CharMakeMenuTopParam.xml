<?xml version="1.0" encoding="utf-8"?>
<PARAMDEF XmlVersion="2">
  <ParamType>CHARMAKEMENUTOP_PARAM_ST</ParamType>
  <DataVersion>3</DataVersion>
  <BigEndian>False</BigEndian>
  <Unicode>True</Unicode>
  <FormatVersion>203</FormatVersion>
  <Fields>
    <Field Def="s32 commandType">
      <DisplayName>Command Type</DisplayName>
      <Enum>CHARMAKEMENU_CMD_TYPE</Enum>
      <Description>Command type</Description>
      <Minimum>-1</Minimum>
      <Maximum>9999999</Maximum>
      <SortID>10</SortID>
    </Field>
    <Field Def="s32 captionId">
      <DisplayName>Caption ID</DisplayName>
      <Description>ID of the text to be displayed</Description>
      <Minimum>-1</Minimum>
      <Maximum>9999999</Maximum>
      <SortID>20</SortID>
    </Field>
    <Field Def="s32 faceParamId">
      <DisplayName>Face Param ID</DisplayName>
      <Enum>FACE_PARAM</Enum>
      <Description>Face Param ID</Description>
      <Minimum>-1</Minimum>
      <Maximum>9999999</Maximum>
      <SortID>30</SortID>
      <ParamRef1>FaceParam</ParamRef1>
    </Field>
    <Field Def="s32 tableId">
      <DisplayName>Table ID - Male</DisplayName>
      <Description>First ID (male) in the list of items to select. The command type is [Slider: Text ID of minimum label (n) and maximum label (n + 1), Color: ID of color table, Grid or text: First ID of character make list item, Other: Ignore]</Description>
      <Minimum>-1</Minimum>
      <Maximum>9999999</Maximum>
      <SortID>40</SortID>
    </Field>
    <Field Def="s32 viewCondition">
      <DisplayName>View Condition</DisplayName>
      <Enum>CHARMAKEMENU_VIEW_CONDITION</Enum>
      <Description>Conditions for displaying this command</Description>
      <Minimum>-1</Minimum>
      <Maximum>9999999</Maximum>
      <SortID>60</SortID>
    </Field>
    <Field Def="s8 previewMode">
      <DisplayName>Preview Mode</DisplayName>
      <Enum>CHARMAKEMENU_PREVIEW_MODE</Enum>
      <Description>Display mode of the character model displayed in preview</Description>
      <Minimum>-1</Minimum>
      <SortID>70</SortID>
    </Field>
    <Field Def="dummy8 reserved2[3]">
      <DisplayName>reserve</DisplayName>
      <SortID>201</SortID>
    </Field>
    <Field Def="s32 tableId2 = -1">
      <DisplayName>Table ID - Female</DisplayName>
      <Description>For women with table ID. If -1, refer to for men</Description>
      <Minimum>-1</Minimum>
      <Maximum>9999999</Maximum>
      <SortID>50</SortID>
    </Field>
    <Field Def="s32 refFaceParamId = -1">
      <DisplayName>Reference Face Param ID</DisplayName>
      <Enum>FACE_PARAM</Enum>
      <Description>Face param ID of the matching destination for "matching to XX"</Description>
      <Minimum>-1</Minimum>
      <Maximum>9999999</Maximum>
      <SortID>100</SortID>
    </Field>
    <Field Def="s32 refTextId = -1">
      <DisplayName>Reference Text ID</DisplayName>
      <Description>Display text ID for "fit to XX"</Description>
      <Minimum>-1</Minimum>
      <Maximum>9999999</Maximum>
      <SortID>110</SortID>
    </Field>
    <Field Def="s32 helpTextId = -1">
      <DisplayName>Help Text ID</DisplayName>
      <Description>1-line help text ID (-1: Get 1-line help with item text ID). Basically, item text ID = 1 line help text ID, but if you want to overwrite part of it, specify it with this parameter.</Description>
      <Minimum>-1</Minimum>
      <Maximum>9999999</Maximum>
      <SortID>25</SortID>
    </Field>
    <Field Def="u32 unlockEventFlagId">
      <DisplayName>Unlock Event Flag ID</DisplayName>
      <Description>Event flag to unlock this item (0: invalid value). Invalid value will always be unlocked</Description>
      <DisplayFormat>%u</DisplayFormat>
      <Maximum>9999999</Maximum>
      <SortID>200</SortID>
    </Field>
    <Field Def="dummy8 reserved[4]">
      <DisplayName>reserve</DisplayName>
      <DisplayFormat>%d</DisplayFormat>
      <EditFlags>Wrap</EditFlags>
      <SortID>202</SortID>
    </Field>
  </Fields>
</PARAMDEF>