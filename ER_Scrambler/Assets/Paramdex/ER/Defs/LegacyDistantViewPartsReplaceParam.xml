<?xml version="1.0" encoding="utf-8"?>
<PARAMDEF XmlVersion="2">
  <ParamType>LEGACY_DISTANT_VIEW_PARTS_REPLACE_PARAM</ParamType>
  <DataVersion>4</DataVersion>
  <BigEndian>False</BigEndian>
  <Unicode>True</Unicode>
  <FormatVersion>203</FormatVersion>
  <Fields>
    <Field Def="s32 TargetMapId = -1">
      <DisplayName>Target Map ID</DisplayName>
      <Description>Specify the target map ID. Legacy only. (m12_34_56_78 → 12345678). Initially it was only legacy, but it also supports open celestial sphere maps</Description>
      <EditFlags>None</EditFlags>
      <Minimum>-1</Minimum>
      <Maximum>9999999</Maximum>
    </Field>
    <Field Def="u32 TargetEventId">
      <DisplayName>Target Event ID</DisplayName>
      <Description>If you enter the target natural disaster event ID from "300, 310, 311 and 312", it will be [natural disaster replacement data]. If it is 0 or blank, it will be [Regional ID switching data] (SEQ16039).</Description>
      <EditFlags>None</EditFlags>
      <Maximum>999</Maximum>
      <SortID>100</SortID>
    </Field>
    <Field Def="s32 SrcAssetId = -1">
      <DisplayName>Replacement Source Asset ID</DisplayName>
      <Description>Replacement source AssetId: AEG123_456_9999 → 123456</Description>
      <EditFlags>None</EditFlags>
      <Minimum>-1</Minimum>
      <Maximum>999999</Maximum>
      <SortID>200</SortID>
    </Field>
    <Field Def="s32 SrcAssetPartsNo = -1">
      <DisplayName>Replacement Source Asset Part ID</DisplayName>
      <Description>Replacement source PartsNo: AEG123_456_9999 → 9999</Description>
      <EditFlags>None</EditFlags>
      <Minimum>-1</Minimum>
      <Maximum>99999</Maximum>
      <SortID>300</SortID>
    </Field>
    <Field Def="s32 DstAssetId = -1">
      <DisplayName>Replacement Destination Asset ID</DisplayName>
      <Description>Replacement destination AssetId: AEG123_456_9999 → 123456</Description>
      <EditFlags>None</EditFlags>
      <Minimum>-1</Minimum>
      <Maximum>999999</Maximum>
      <SortID>400</SortID>
    </Field>
    <Field Def="s32 DstAssetPartsNo = -1">
      <DisplayName>Replacement Destination Asset Part ID</DisplayName>
      <Description>Replacement destination PartsNo: AEG123_456_9999 → 9999</Description>
      <EditFlags>None</EditFlags>
      <Minimum>-1</Minimum>
      <Maximum>99999</Maximum>
      <SortID>500</SortID>
    </Field>
    <Field Def="s32 SrcAssetIdRangeMin = -1">
      <DisplayName>Replacement Source Asset ID - Min Range</DisplayName>
      <Description>Replacement source asset ID range specification Min</Description>
      <EditFlags>None</EditFlags>
      <Minimum>-1</Minimum>
      <Maximum>999999</Maximum>
      <SortID>600</SortID>
    </Field>
    <Field Def="s32 SrcAssetIdRangeMax = -1">
      <DisplayName>Replacement Source Asset ID - Max Range</DisplayName>
      <Description>Replacement source asset ID range specification Max</Description>
      <EditFlags>None</EditFlags>
      <Minimum>-1</Minimum>
      <Maximum>999999</Maximum>
      <SortID>700</SortID>
    </Field>
    <Field Def="s32 DstAssetIdRangeMin = -1">
      <DisplayName>Replacement Destination Asset ID - Min Range</DisplayName>
      <Description>Replacement destination asset ID range specification Min</Description>
      <EditFlags>None</EditFlags>
      <Minimum>-1</Minimum>
      <Maximum>999999</Maximum>
      <SortID>800</SortID>
    </Field>
    <Field Def="s32 DstAssetIdRangeMax = -1">
      <DisplayName>Replacement Destination Asset ID - Max Range</DisplayName>
      <Description>Replacement destination asset ID range specification Max</Description>
      <EditFlags>None</EditFlags>
      <Minimum>-1</Minimum>
      <Maximum>999999</Maximum>
      <SortID>900</SortID>
    </Field>
    <Field Def="s8 LimitedMapRegionId0 = -1">
      <DisplayName>Limited Map Region ID [0]</DisplayName>
      <Description>MapGD Local ID restriction 0: Valid only when "Natural disaster event ID" is 0. The part is valid only with the MapGD local ID you set (SEQ16039).</Description>
      <EditFlags>None</EditFlags>
      <Minimum>-1</Minimum>
      <Maximum>99</Maximum>
      <SortID>1000</SortID>
    </Field>
    <Field Def="s8 LimitedMapRegionId1 = -1">
      <DisplayName>Limited Map Region ID [1]</DisplayName>
      <Description>MapGD Local ID Restriction 1: Valid only when "Natural Disaster Event ID" is 0. The part is valid only with the MapGD local ID you set (SEQ16039).</Description>
      <EditFlags>None</EditFlags>
      <Minimum>-1</Minimum>
      <Maximum>99</Maximum>
      <SortID>1001</SortID>
    </Field>
    <Field Def="s8 LimitedMapRegionId2 = -1">
      <DisplayName>Limited Map Region ID [2]</DisplayName>
      <Description>MapGD Local ID restriction 2: Valid only when "Natural disaster event ID" is 0. The part is valid only with the MapGD local ID you set (SEQ16039).</Description>
      <EditFlags>None</EditFlags>
      <Minimum>-1</Minimum>
