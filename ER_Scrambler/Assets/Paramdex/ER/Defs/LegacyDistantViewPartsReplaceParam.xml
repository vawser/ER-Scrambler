      <EditFlags>None</EditFlags>
      <Minimum>-1</Minimum>
      <Maximum>99999</Maximum>
      <SortID>1101</SortID>
    </Field>
    <Field Def="s32 LimitedMapRegioAssetIdRangeMin = -1">
      <DisplayName>Limited Map Region Asset ID - Range Min</DisplayName>
      <Description>MapGD Valid asset specification with local ID: Asset ID range specification Min</Description>
      <EditFlags>None</EditFlags>
      <Minimum>-1</Minimum>
      <Maximum>999999</Maximum>
      <SortID>1200</SortID>
    </Field>
    <Field Def="s32 LimitedMapRegioAssetIdRangeMax = -1">
      <DisplayName>Limited Map Region Asset ID - Range Max</DisplayName>
