      <Minimum>0</Minimum>
      <Maximum>100</Maximum>
      <SortID>54000</SortID>
    </Field>
    <Field Def="s8 fireDamageDamp">
      <DisplayName>Damage Falloff %: Fire</DisplayName>
      <Description>Correction value that decreases in 1 second after the attenuation distance.</Description>
      <Minimum>0</Minimum>
      <Maximum>100</Maximum>
      <SortID>55000</SortID>
    </Field>
    <Field Def="s8 thunderDamageDamp">
      <DisplayName>Damage Falloff %: Lightning</DisplayName>
      <Description>Correction value that decreases in 1 second after the attenuation distance.</Description>
      <Minimum>0</Minimum>
      <Maximum>100</Maximum>
      <SortID>56000</SortID>
    </Field>
    <Field Def="s8 staminaDamp">
      <DisplayName>Damage Falloff %: Stamina</DisplayName>
      <Description>Correction value that decreases in 1 second after the attenuation distance.</Description>
      <Minimum>0</Minimum>
      <Maximum>100</Maximum>
      <SortID>57000</SortID>
    </Field>
    <Field Def="s8 knockbackDamp">
      <DisplayName>Damage Falloff %: Knockback</DisplayName>
      <Description>Correction value that decreases in 1 second after the attenuation distance.</Description>
      <Minimum>0</Minimum>
      <Maximum>100</Maximum>
      <SortID>58000</SortID>
    </Field>
    <Field Def="s8 shootAngleXZ">
      <DisplayName>Launch Elevation Angle</DisplayName>
      <Description>Additional elevation angle from the horizontal.</Description>
      <Minimum>-90</Minimum>
      <Maximum>90</Maximum>
      <SortID>26000</SortID>
    </Field>
    <Field Def="u8 lockShootLimitAng">
      <DisplayName>Lock Direction Limit Angle</DisplayName>
      <Description>Limit angle when facing the lock direction</Description>
      <Maximum>180</Maximum>
      <SortID>42000</SortID>
    </Field>
    <Field Def="dummy8 pad2[1]">
      <DisplayName>pad</DisplayName>
      <DisplayFormat>%d</DisplayFormat>
      <EditFlags>Wrap</EditFlags>
      <SortID>99201</SortID>
    </Field>
    <Field Def="u8 prevVelocityDirRate">
      <DisplayName>Previous Velocity Direction Rate</DisplayName>
      <Description>Ratio to add the previous movement direction to the current direction when the sliding bullet hits the wall</Description>
      <Maximum>100</Maximum>
      <SortID>46500</SortID>
    </Field>
    <Field Def="u8 atkAttribute = 254">
      <DisplayName>Attack Attribute</DisplayName>
      <Enum>ATKPARAM_ATKATTR_TYPE</Enum>
      <Description>Set the physical attributes to set for the bullet</Description>
      <SortID>6000</SortID>
    </Field>
    <Field Def="u8 spAttribute = 254">
      <DisplayName>Special Attribute</DisplayName>
      <Enum>ATKPARAM_SPATTR_TYPE</Enum>
      <Description>Set special attributes to set for bullets</Description>
      <SortID>7000</SortID>
    </Field>
    <Field Def="u8 Material_AttackType = 254">
      <DisplayName>Material Attack Type</DisplayName>
      <Enum>BEHAVIOR_ATK_TYPE</Enum>
      <Description>Specify what the attack attribute is</Description>
      <SortID>8000</SortID>
    </Field>
    <Field Def="u8 Material_AttackMaterial = 254">
      <DisplayName>Material Attack Material</DisplayName>
      <Enum>WEP_MATERIAL_ATK</Enum>
      <Description>Used for SFX / SE during attack</Description>
      <SortID>9000</SortID>
    </Field>
    <Field Def="u8 isPenetrateChr:1">
      <DisplayName>Is Penetrate Character</DisplayName>
      <Enum>ON_OFF</Enum>
      <Description>If it is ON, it will penetrate without landing when it hits the character.</Description>
      <Maximum>1</Maximum>
      <SortID>19000</SortID>
    </Field>
    <Field Def="u8 isPenetrateObj:1">
      <DisplayName>Is Penetrate Object</DisplayName>
      <Enum>ON_OFF</Enum>
      <Description>If it is ON, it will penetrate without landing when hitting a dynamic / partial destruction asset.</Description>
      <Maximum>1</Maximum>
      <SortID>19100</SortID>
    </Field>
    <Field Def="dummy8 pad:6">
      <DisplayName>pad</DisplayName>
      <DisplayFormat>%d</DisplayFormat>
      <EditFlags>Wrap</EditFlags>
      <SortID>99202</SortID>
    </Field>
    <Field Def="u8 launchConditionType">
      <DisplayName>Launch Condition Type</DisplayName>
      <Enum>BULLET_LAUNCH_CONDITION_TYPE</Enum>
      <Description>Specify the condition to judge whether to generate a bullet when it lands or the life is extinguished</Description>
      <SortID>12000</SortID>
    </Field>
    <Field Def="u8 FollowType:3">
      <DisplayName>Follow Type</DisplayName>
      <Enum>BULLET_FOLLOW_TYPE</Enum>
      <Description>Follow-up type. "Do not follow" is the default.</Description>
      <SortID>14000</SortID>
    </Field>
    <Field Def="u8 EmittePosType:3">
      <DisplayName>Emitter Source Type</DisplayName>
      <Enum>BULLET_EMITTE_POS_TYPE</Enum>
      <Description>Source type. Usually from Damipoli. (Introduced to judge meteo)</Description>
      <Maximum>7</Maximum>
      <SortID>13000</SortID>
    </Field>
    <Field Def="u8 isAttackSFX:1">
      <DisplayName>Is Persistent Attack SFX</DisplayName>
      <Enum>ON_OFF</Enum>
      <Description>Set whether bullets such as arrows will remain stuck in the character</Description>
      <Maximum>1</Maximum>
      <SortID>5000</SortID>
    </Field>
    <Field Def="u8 isEndlessHit:1">
      <DisplayName>Is Continuous Hit</DisplayName>
      <Enum>ON_OFF</Enum>
      <Description>Do you keep hitting?</Description>
      <Maximum>1</Maximum>
      <SortID>15000</SortID>
    </Field>
    <Field Def="u8 isPenetrateMap:1">
      <DisplayName>Is Penetrate Map</DisplayName>
      <Enum>ON_OFF</Enum>
      <Description>If it is ON, it will penetrate without landing when hitting a hit / static asset.</Description>
      <Maximum>1</Maximum>
      <SortID>20000</SortID>
    </Field>
    <Field Def="u8 isHitBothTeam:1">
      <DisplayName>Is Hit Both Teams</DisplayName>
      <Enum>ON_OFF</Enum>
      <Description>Are you an enemy or an ally? (Not a wandering ghost)</Description>
      <Maximum>1</Maximum>
      <SortID>21000</SortID>
    </Field>
    <Field Def="u8 isUseSharedHitList:1">
      <DisplayName>Is Use Shared Hit List</DisplayName>
      <Enum>ON_OFF</Enum>
      <Description>Specify whether to share the hit list</Description>
      <Maximum>1</Maximum>
      <SortID>16000</SortID>
    </Field>
    <Field Def="u8 isUseMultiDmyPolyIfPlace:1">
      <DisplayName>Use Multiple Dummy Polys for Placement</DisplayName>
      <Enum>ON_OFF</Enum>
      <Description>Do you use the same Damipoly ID more than once when placing bullets?</Description>
      <Maximum>1</Maximum>
      <SortID>22000</SortID>
    </Field>
    <Field Def="u8 isHitOtherBulletForceEraseA:1">
      <DisplayName>Force Erase Bullet A if Hit by Other Bullet</DisplayName>
      <Description>Does it hit other bullets forced erasure A?</Description>
      <Maximum>1</Maximum>
      <SortID>62200</SortID>
      <Enum>ON_OFF</Enum>
    </Field>
    <Field Def="u8 isHitOtherBulletForceEraseB:1">
      <DisplayName>Force Erase Bullet B if Hit by Other Bullet</DisplayName>
      <Description>Does it hit the other bullet forced erasure B?</Description>
      <Maximum>1</Maximum>
      <SortID>62300</SortID>
      <Enum>ON_OFF</Enum>
    </Field>
    <Field Def="u8 isHitForceMagic:1">
      <DisplayName>Is Bullet deflected by Magic</DisplayName>
      <Description>Do you hit the force magic?</Description>
      <Maximum>1</Maximum>
      <SortID>62000</SortID>
      <Enum>ON_OFF</Enum>
    </Field>
    <Field Def="u8 isIgnoreSfxIfHitWater:1">
      <DisplayName>Is SFX ignored on Water Hit</DisplayName>
      <Description>Should I ignore the effect if it hits the surface of the water?</Description>
      <Maximum>1</Maximum>
      <SortID>63000</SortID>
      <Enum>ON_OFF</Enum>
    </Field>
    <Field Def="u8 isIgnoreMoveStateIfHitWater:1">
      <DisplayName>Is Move State Ignored on Water Hit</DisplayName>
      <Description>Whether to ignore the state transition even if it hits water</Description>
      <Maximum>1</Maximum>
      <SortID>64000</SortID>
      <Enum>ON_OFF</Enum>
    </Field>
    <Field Def="u8 isHitDarkForceMagic:1">
      <DisplayName>Is Bullet deflected by Bullet Deflection</DisplayName>
      <Description>Do you hit the dark force magic?</Description>
      <Maximum>1</Maximum>
      <SortID>62000</SortID>
      <Enum>ON_OFF</Enum>
    </Field>
    <Field Def="u8 dmgCalcSide:2">
      <DisplayName>Damage Calculation Side</DisplayName>
      <Enum>DMG_CALC_SIDE_TYPE</Enum>
      <Description>Damage calculation side. During multiplayer, the damage calculation is for switching between the giving side and the receiving side.</Description>
      <Maximum>1</Maximum>
      <SortID>70000</SortID>
    </Field>
    <Field Def="u8 isEnableAutoHoming:1">
      <DisplayName>Is Auto Homing Enabled</DisplayName>
      <Enum>ON_OFF</Enum>
