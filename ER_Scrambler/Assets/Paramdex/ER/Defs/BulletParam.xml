      <Maximum>1</Maximum>
      <SortID>14500</SortID>
    </Field>
    <Field Def="u8 isSyncBulletCulcDumypolyPos:1">
      <DisplayName>Is Emitter Source Synced with Bullet</DisplayName>
      <Enum>ON_OFF</Enum>
