      <Minimum>0</Minimum>
      <Maximum>999</Maximum>
      <SortID>81000</SortID>
    </Field>
    <Field Def="f32 intervalCreateTimeMax">
      <DisplayName>Bullet Emitter: Maximum Shoot Interval</DisplayName>
