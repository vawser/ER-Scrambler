      <DisplayName>Damage Falloff %: Physical</DisplayName>
      <Description>Correction value that decreases in 1 second after the attenuation distance.</Description>
      <Minimum>0</Minimum>
      <Maximum>100</Maximum>
      <SortID>53000</SortID>
    </Field>
    <Field Def="s8 spelDamageDamp">
      <DisplayName>Damage Falloff %: Magic</DisplayName>
