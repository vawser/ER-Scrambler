      <Description>Upper limit of random number of firing angle (0 to 360)</Description>
      <Minimum>0</Minimum>
      <Maximum>360</Maximum>
      <SortID>25100</SortID>
    </Field>
    <Field Def="f32 shootAngleXMaxRandom">
      <DisplayName>Random Elevation Angle - Max</DisplayName>
      <Description>Upper limit of firing elevation random number (0 to 360)</Description>
      <Minimum>0</Minimum>
      <Maximum>360</Maximum>
      <SortID>27100</SortID>
    </Field>
