<?xml version="1.0" encoding="utf-8"?>
<PARAMDEF XmlVersion="2">
  <ParamType>BULLET_PARAM_ST</ParamType>
  <DataVersion>4</DataVersion>
  <BigEndian>False</BigEndian>
  <Unicode>True</Unicode>
  <FormatVersion>203</FormatVersion>
  <Fields>
    <Field Def="s32 atkId_Bullet = -1">
      <DisplayName>AtkParam ID</DisplayName>
      <Description>Register each attack parameter ID. → Attack type / Attack material / Physical attack power / Magic attack power / Stamina attack power / Knockback distance.</Description>
      <Minimum>-1</Minimum>
      <Maximum>9999999</Maximum>
      <SortID>1000</SortID>
      <ParamRef1>AtkParam_Pc</ParamRef1>
      <ParamRef2>AtkParam_Npc</ParamRef2>
    </Field>
    <Field Def="s32 sfxId_Bullet = -1">
      <DisplayName>Projectile SFX ID</DisplayName>
      <Description>Enter the SFX ID for [Bullet]. -1 does not occur.</Description>
      <Minimum>-1</Minimum>
      <Maximum>9999999</Maximum>
      <SortID>2000</SortID>
    </Field>
    <Field Def="s32 sfxId_Hit = -1">
      <DisplayName>Impact SFX ID</DisplayName>
      <Description>[Impact] Enter the SFX ID. -1 does not occur.</Description>
      <Minimum>-1</Minimum>
      <Maximum>9999999</Maximum>
      <SortID>3000</SortID>
    </Field>
    <Field Def="s32 sfxId_Flick = -1">
      <DisplayName>Repel SFX ID</DisplayName>
      <Description>[At the time of repelling] Enter the SFX ID. -1 does not occur.</Description>
      <Minimum>-1</Minimum>
      <Maximum>9999999</Maximum>
      <SortID>4000</SortID>
    </Field>
    <Field Def="f32 life = -1">
      <DisplayName>Duration</DisplayName>
      <Description>Time for missiles to continue to exist (-1 is infinite).</Description>
      <Minimum>-1</Minimum>
      <Maximum>99999</Maximum>
      <SortID>29000</SortID>
    </Field>
    <Field Def="f32 dist">
      <DisplayName>Attenuation Range</DisplayName>
      <Description>Distance at which attenuation begins (not actual flight distance).</Description>
      <Minimum>0</Minimum>
      <Maximum>99999</Maximum>
      <Increment>0.1</Increment>
      <SortID>33000</SortID>
    </Field>
    <Field Def="f32 shootInterval">
      <DisplayName>Shoot Interval</DisplayName>
      <Description>Specify how many seconds the missile will be fired.</Description>
      <Minimum>0</Minimum>
      <Maximum>99</Maximum>
      <SortID>28000</SortID>
    </Field>
    <Field Def="f32 gravityInRange">
      <DisplayName>Gravity: In Range</DisplayName>
      <Description>Downward gravity within range.</Description>
      <Minimum>-999</Minimum>
      <Maximum>999</Maximum>
      <Increment>0.1</Increment>
      <SortID>34000</SortID>
    </Field>
    <Field Def="f32 gravityOutRange">
      <DisplayName>Gravity: Out of Range</DisplayName>
      <Description>Gravity applied downward when damping begins (expresses the feeling of falling down.</Description>
      <Minimum>-999</Minimum>
      <Maximum>999</Maximum>
      <Increment>0.1</Increment>
      <SortID>35000</SortID>
    </Field>
    <Field Def="f32 hormingStopRange">
      <DisplayName>Homing Stop Range</DisplayName>
      <Description>The distance to the target to stop the guidance. A parameter that prevents you from hitting too much with a guided bullet.</Description>
      <Minimum>0</Minimum>
      <Maximum>999999</Maximum>
      <Increment>0.1</Increment>
      <SortID>45000</SortID>
    </Field>
    <Field Def="f32 initVellocity">
      <DisplayName>Initial Velocity</DisplayName>
      <Description>Initial speed of SFX.</Description>
      <Minimum>0</Minimum>
      <Maximum>999</Maximum>
      <Increment>0.1</Increment>
      <SortID>36000</SortID>
    </Field>
    <Field Def="f32 accelInRange">
      <DisplayName>Acceleration: In Range</DisplayName>
      <Description>Acceleration within SFX range.</Description>
      <Minimum>-999</Minimum>
      <Maximum>999</Maximum>
      <Increment>0.1</Increment>
      <SortID>40000</SortID>
    </Field>
    <Field Def="f32 accelOutRange">
      <DisplayName>Acceleration: Out of Range</DisplayName>
      <Description>Acceleration when SFX goes out of range.</Description>
      <Minimum>-999</Minimum>
      <Maximum>999</Maximum>
      <Increment>0.1</Increment>
      <SortID>41000</SortID>
    </Field>
    <Field Def="f32 maxVellocity">
      <DisplayName>Max Velocity</DisplayName>
      <Description>maximum speed.</Description>
      <Minimum>0</Minimum>
      <Maximum>999</Maximum>
      <Increment>0.1</Increment>
      <SortID>38000</SortID>
    </Field>
    <Field Def="f32 minVellocity">
      <DisplayName>Min Velocity</DisplayName>
      <Description>Minimum guaranteed speed.</Description>
      <Minimum>0</Minimum>
      <Maximum>999</Maximum>
      <Increment>0.1</Increment>
      <SortID>37000</SortID>
    </Field>
    <Field Def="f32 accelTime">
      <DisplayName>Acceleration Duration</DisplayName>
      <Description>Until this time, do not accelerate (make sure you can shoot magic like rockets).</Description>
      <Minimum>0</Minimum>
      <Maximum>99</Maximum>
      <SortID>39000</SortID>
    </Field>
    <Field Def="f32 homingBeginDist">
      <DisplayName>Homing Start Distance</DisplayName>
      <Description>How many meters should the guidance start?</Description>
      <Minimum>0</Minimum>
      <Maximum>999</Maximum>
      <Increment>0.1</Increment>
      <SortID>44000</SortID>
    </Field>
    <Field Def="f32 hitRadius = -1">
      <DisplayName>Initial Hit Radius</DisplayName>
      <Description>Set the radius of the hit ball.</Description>
      <Minimum>-1</Minimum>
      <Maximum>999</Maximum>
      <SortID>30000</SortID>
    </Field>
    <Field Def="f32 hitRadiusMax = -1">
      <DisplayName>Final Hit Radius</DisplayName>
      <Description>Maximum radius of the hit sphere (If -1, make it the same as the initial radius / default)</Description>
      <Minimum>-1</Minimum>
      <Maximum>999</Maximum>
      <SortID>31000</SortID>
    </Field>
    <Field Def="f32 spreadTime">
      <DisplayName>Radius Spread Time</DisplayName>
      <Description>The time when the radius of the range expands to a small extent.</Description>
      <Minimum>0</Minimum>
      <Maximum>99</Maximum>
      <SortID>32000</SortID>
    </Field>
    <Field Def="f32 expDelay">
      <DisplayName>Explosion Delay</DisplayName>
      <Description>Time from landing to explosion (if 0, it explodes immediately).</Description>
      <Minimum>0</Minimum>
      <Maximum>99</Maximum>
      <SortID>59000</SortID>
    </Field>
    <Field Def="f32 hormingOffsetRange">
      <DisplayName>Homing Offset Range</DisplayName>
      <Description>It is accurate if it is 0. At the time of shooting, each component of XYZ should be aimed by shifting this amount.</Description>
      <Minimum>0</Minimum>
      <Maximum>999</Maximum>
      <Increment>0.1</Increment>
      <SortID>46000</SortID>
    </Field>
    <Field Def="f32 dmgHitRecordLifeTime">
      <DisplayName>Damage Hit Duration</DisplayName>
      <Description>Damage hit history survival time [sec] (&lt;= 0.0f: indefinite)</Description>
      <Minimum>0</Minimum>
      <Maximum>9999</Maximum>
      <Increment>0.1</Increment>
      <SortID>17000</SortID>
    </Field>
    <Field Def="f32 externalForce">
      <DisplayName>External Force in Bullet Direction</DisplayName>
      <Description>External force applied in the direction of shooting. (Y-axis is removed)</Description>
      <Minimum>0</Minimum>
      <Maximum>10</Maximum>
      <Increment>0.001</Increment>
      <SortID>35500</SortID>
    </Field>
    <Field Def="s32 spEffectIDForShooter = -1">
      <DisplayName>Shooter SpEffect ID</DisplayName>
      <Description>Special effects on the person who shot</Description>
      <Minimum>-1</Minimum>
      <Maximum>9999999</Maximum>
      <SortID>47000</SortID>
      <ParamRef1>SpEffectParam</ParamRef1>
    </Field>
    <Field Def="s32 autoSearchNPCThinkID">
      <DisplayName>Bullet AI</DisplayName>
      <Description>Parameters used by the funnel to search for the target</Description>
      <Minimum>-1</Minimum>
      <Maximum>9999999</Maximum>
      <SortID>60000</SortID>
    </Field>
    <Field Def="s32 HitBulletID = -1">
      <DisplayName>Hit Bullet ID</DisplayName>
      <Description>Specify the ID when generating a new bullet parameter from the bullet parameter</Description>
      <Minimum>-1</Minimum>
      <Maximum>9999999</Maximum>
      <SortID>11000</SortID>
      <ParamRef1>Bullet</ParamRef1>
    </Field>
    <Field Def="s32 spEffectId0 = -1">
      <DisplayName>Target SpEffect ID [0]</DisplayName>
      <Description>Register each special effect parameter ID. → General special effects.</Description>
      <Minimum>-1</Minimum>
      <Maximum>9999999</Maximum>
      <SortID>48000</SortID>
      <ParamRef1>SpEffectParam</ParamRef1>
    </Field>
    <Field Def="s32 spEffectId1 = -1">
      <DisplayName>Target SpEffect ID [1]</DisplayName>
      <Description>Register each special effect parameter ID. → General special effects.</Description>
      <Minimum>-1</Minimum>
      <Maximum>9999999</Maximum>
      <SortID>49000</SortID>
      <ParamRef1>SpEffectParam</ParamRef1>
    </Field>
    <Field Def="s32 spEffectId2 = -1">
      <DisplayName>Target SpEffect ID [2]</DisplayName>
      <Description>Register each special effect parameter ID. → General special effects.</Description>
      <Minimum>-1</Minimum>
      <Maximum>9999999</Maximum>
      <SortID>50000</SortID>
      <ParamRef1>SpEffectParam</ParamRef1>
    </Field>
    <Field Def="s32 spEffectId3 = -1">
      <DisplayName>Target SpEffect ID [3]</DisplayName>
      <Description>Register each special effect parameter ID. → General special effects.</Description>
      <Minimum>-1</Minimum>
      <Maximum>9999999</Maximum>
      <SortID>51000</SortID>
      <ParamRef1>SpEffectParam</ParamRef1>
    </Field>
    <Field Def="s32 spEffectId4 = -1">
      <DisplayName>Target SpEffect ID [4]</DisplayName>
      <Description>Register each special effect parameter ID. → General special effects.</Description>
      <Minimum>-1</Minimum>
      <Maximum>9999999</Maximum>
      <SortID>52000</SortID>
      <ParamRef1>SpEffectParam</ParamRef1>
    </Field>
    <Field Def="u16 numShoot">
      <DisplayName>Bullet Count</DisplayName>
      <Description>The number of missiles fired at one time.</Description>
      <Maximum>99</Maximum>
      <SortID>23000</SortID>
    </Field>
    <Field Def="s16 homingAngle">
      <DisplayName>Homing Angle</DisplayName>
      <Description>How many corrections per second? ..</Description>
      <Minimum>0</Minimum>
      <SortID>43000</SortID>
    </Field>
    <Field Def="s16 shootAngle">
      <DisplayName>Launch Angle</DisplayName>
      <Description>Specify how many times the missile is fired forward.</Description>
      <Minimum>-360</Minimum>
      <Maximum>360</Maximum>
      <SortID>24000</SortID>
    </Field>
    <Field Def="s16 shootAngleInterval">
      <DisplayName>Launch Angle Interval</DisplayName>
      <Description>When firing multiple missiles, specify how often to fire them. (Y-axis)</Description>
      <Minimum>-360</Minimum>
      <Maximum>360</Maximum>
      <SortID>25000</SortID>
    </Field>
    <Field Def="s16 shootAngleXInterval">
      <DisplayName>Launch Elevation Interval</DisplayName>
      <Description>When firing multiple missiles, specify how often to fire them. (X-axis)</Description>
      <Minimum>-360</Minimum>
      <Maximum>360</Maximum>
      <SortID>27000</SortID>
    </Field>
