<?xml version="1.0" encoding="utf-8"?>
<PARAMDEF XmlVersion="2">
  <ParamType>CS_LIGHTING_QUALITY_DETAIL</ParamType>
  <DataVersion>1</DataVersion>
  <BigEndian>False</BigEndian>
  <Unicode>True</Unicode>
  <FormatVersion>203</FormatVersion>
  <Fields>
    <Field Def="f32 localLightDistFactor = 0.75">
      <DisplayName>Local Light Distance Factor</DisplayName>
      <Description>Local light effective distance coefficient (smaller, it disappears at a short distance)</Description>
      <EditFlags>None</EditFlags>
      <Minimum>0.5</Minimum>
      <Maximum>1.2</Maximum>
      <SortID>1</SortID>
    </Field>
    <Field Def="u8 localLightShadowEnabled = 1">
      <DisplayName>Local Light Shadow Enabled</DisplayName>
      <Enum>ON_OFF</Enum>
      <Description>Local light shadow enabled</Description>
      <DisplayFormat>%u</DisplayFormat>
      <EditFlags>None</EditFlags>
      <Maximum>1</Maximum>
      <SortID>2</SortID>
    </Field>
    <Field Def="u8 forwardPassLightingEnabled = 1">
      <DisplayName>Forward Pass Write Enabled</DisplayName>
      <Enum>ON_OFF</Enum>
      <Description>Forward pass writing enabled</Description>
      <DisplayFormat>%u</DisplayFormat>
      <EditFlags>None</EditFlags>
      <Maximum>1</Maximum>
      <SortID>3</SortID>
    </Field>
    <Field Def="u8 localLightShadowSpecLevelMax = 1">
      <DisplayName>Local Light Shadow Specular Level</DisplayName>
      <Description>Local light shadow spec level. The larger the value, the more light sources will be shadowed.</Description>
      <DisplayFormat>%u</DisplayFormat>
      <EditFlags>None</EditFlags>
      <Minimum>1</Minimum>
      <Maximum>5</Maximum>
      <SortID>4</SortID>
    </Field>
    <Field Def="dummy8 dmy[1]">
      <DisplayName>dmy</DisplayName>
      <SortID>6</SortID>
    </Field>
  </Fields>
</PARAMDEF>