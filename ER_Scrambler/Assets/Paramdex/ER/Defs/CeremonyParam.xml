<?xml version="1.0" encoding="utf-8"?>
<PARAMDEF XmlVersion="2">
  <ParamType>CEREMONY_PARAM_ST</ParamType>
  <DataVersion>1</DataVersion>
  <BigEndian>False</BigEndian>
  <Unicode>True</Unicode>
  <FormatVersion>203</FormatVersion>
  <Fields>
    <Field Def="s32 eventLayerId">
      <DisplayName>Event Layer ID</DisplayName>
      <Description>Event maker layer ID</Description>
      <Minimum>0</Minimum>
      <Maximum>9</Maximum>
      <SortID>1000</SortID>
    </Field>
    <Field Def="s32 mapStudioLayerId">
      <DisplayName>Map Studio Layer ID</DisplayName>
      <Description>Map Studio Layer ID</Description>
      <Minimum>0</Minimum>
      <Maximum>31</Maximum>
      <SortID>1100</SortID>
    </Field>
    <Field Def="s32 multiPlayAreaOffset">
      <DisplayName>Play Region ID Offset</DisplayName>
      <Description>Multiplayer area ID offset. For example, if you enter "100", the multiplayer area ID will be offset by "100".</Description>
      <Minimum>0</Minimum>
      <Maximum>1999999999</Maximum>
      <SortID>1200</SortID>
    </Field>
    <Field Def="s32 overrideMapPlaceNameId = -1">
      <DisplayName>Override Map Place Name ID</DisplayName>
      <Description>Override the map name ID_place name display with the specified ID. -1: No overwrite, -2 or less, 0 or more: Overwrite the ID.</Description>
      <SortID>1300</SortID>
    </Field>
    <Field Def="s32 overrideSaveMapNameId = -1">
      <DisplayName>Override Save Map Name ID</DisplayName>
      <Description>Map name ID_Overwrites the save data display with the specified ID. -1: No overwrite, -2 or less, 0 or more: Overwrite the ID.</Description>
      <SortID>1400</SortID>
    </Field>
    <Field Def="dummy8 pad2[16]">
      <DisplayName>pad2</DisplayName>
      <SortID>1401</SortID>
    </Field>
  </Fields>
</PARAMDEF>